<div class="holder_right">
  <div class="header">
    <div class="header_content">
      <h1><%=h @page.title %></h1>
    </div>
    <div class="header_right">
    </div>
  </div>
  <div class="main_content">
    <% for part in @page_parts %>
        <% unless part.was_deleted %>
            <%= markdown(part.body) %>
        <% end %>
    <% end %>
  </div>
</div>
<div class="footer">
  <div class="footer_left">
    &nbsp;
  </div>
  <div class="footer_right_light">
    &nbsp;
  </div>
</div>

        
